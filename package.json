{
    "name": "wyzetalk-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.cjs",
        "preview": "vite preview",
        "test": "vitest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "vitest"
        ]
    },
    "dependencies": {
        "@emotion/css": "^11.11.2",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.10",
        "@mui/material": "^5.15.10",
        "@reduxjs/toolkit": "^2.2.1",
        "@types/react-virtualized-auto-sizer": "^1.0.4",
        "@types/react-window": "^1.8.8",
        "@types/react-window-infinite-loader": "^1.0.9",
        "eslint-plugin-react": "^7.33.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-lorem-ipsum": "^1.4.10",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.1",
        "react-virtualized-auto-sizer": "^1.0.23",
        "react-window": "^1.8.10",
        "react-window-infinite-loader": "^1.0.9",
        "redux-thunk": "^3.1.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.202",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "lodash": "^4.17.21",
        "prettier": "^3.2.5",
        "sass": "^1.71.1",
        "typescript": "^5.2.2",
        "vite": "^5.1.0",
        "vitest": "^1.3.1"
    }
}
